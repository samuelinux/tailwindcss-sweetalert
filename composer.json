{
    "name": "samuelpereiramachado/tailwindcss-sweetalert",
    "description": "TailwindCSS + SweetAlert2 auto-injected for Laravel TALL Stack. Zero configuration.",
    "keywords": ["laravel", "tailwindcss", "sweetalert2", "livewire", "tall-stack"],
    "homepage": "https://github.com/samuelinux/tailwindcss-sweetalert",
    "type": "library",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Pereira Machado",
            "email": "samuelpereiramachado@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SamuelPereiraMachado\\TailwindcssSweetalert\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SamuelPereiraMachado\\TailwindcssSweetalert\\TailwindcssSweetalertServiceProvider"
            ]
        }
    }
}
