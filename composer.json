{
    "name": "samuelpereiramachado/tailwindcss-sweetalert",
    "description": "TailwindCSS + SweetAlert2 auto-injected for Laravel TALL Stack. Zero configuration.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Pereira Machado"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SamuelPereiraMachado\\TailwindcssSweetalert\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SamuelPereiraMachado\\TailwindcssSweetalert\\TailwindcssSweetalertServiceProvider"
            ]
        }
    }
}
